<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

    <GenerateSaveInfoNodeParser>true</GenerateSaveInfoNodeParser>
    <!-- 去掉元数据 -->
    <IlcDisableReflection>true</IlcDisableReflection>
    <PublishAot>true</PublishAot>
  </PropertyGroup>

  <Import Project="..\DotNetCampus.Storage.Lib\Assets\Package.props" />

  <ItemGroup>
    <ProjectReference Include="..\DotNetCampus.Storage.Analyzer\DotNetCampus.Storage.Analyzer.csproj" OutputItemType="Analyzer" ReferenceOutputAssembly="false" />
    <ProjectReference Include="..\DotNetCampus.Storage.Lib\DotNetCampus.Storage.Lib.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Asserts\TestFiles\Test.opc">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
