<Window x:Class="WeefemwidaWawjiwewhe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WeefemwidaWawjiwewhe"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dataGrid" IsReadOnly="True">
            <DataGrid.Columns>
                <DataGridTextColumn Header="A" Binding="{Binding Path=A}"/>
                <DataGridTextColumn Header="B" Binding="{Binding Path=B}"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
